#include <bits/stdc++.h>

using namespace std;



int main()
{
    int N;
    cin >> N;
    if (N<1 | N>100)

      {
        exit(0);
      }

    if (N%2==1)
    {
        printf("Weird");
    }
    if (N%2==0)
    {
        if (N>=2 & N<=5)
        {
            printf("Not Weird");
        }
        if (N>=6 & N<=20)
        {
            printf("Weird");
        }
        if (N>20)
        {
            printf("Not Weird");
        }
    }


    return 0;
}
