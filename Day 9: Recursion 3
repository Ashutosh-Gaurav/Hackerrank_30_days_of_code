#include <bits/stdc++.h>

using namespace std;

// Complete the factorial function below.
int factorial(int n)
{
    int k;
    if (n>1)
    {
        k=(n*factorial(n-1));
    }
    else if (n=1)
    {
        k=(1);
    }
    return k;
}

int main()
{


    int n;
    cin >> n;


    int result = factorial(n);

    cout << result << "\n";



    return 0;
}
